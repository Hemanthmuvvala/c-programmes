
#include<stdio.h>

 int main()
{
    char s1[10],s2[10];
    int i,j,l,t=0;
    scanf("%s",s1);
    for(i=0; s1[i]!='\0'; i++)
        l++;
        
        printf("length of the string=%d",l);
    for(i=0,j=l-1; s2[j]!='\0',s1[i]!='\0'; i++,j--)
     {   s2[j]=s1[i];
        
    }
    printf("\n%s",s2);
    for(i=0,j=0; s2[j]!='\0',s1[i]!='\0'; i++,j++)
    {if(s1[i]==s2[j])
    t++;
    }
    if(t==l)
    printf("\npalindrome");
    else
    printf ("\nnot a palindrome");
}
